{
  "source": {
    "include": ["src"],
    "includePattern": ".tsx?$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "templates/default",
    "encoding": "utf8",
    "private": false
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "includeDate": false,
      "outputSourceFiles": false
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
